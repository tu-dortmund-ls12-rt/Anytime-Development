    "version": "2.0.0",
    "tasks": [
        {
            "label": "ROS2-Build",
            "type": "shell",
            "command": "cd ${workspaceFolder}/packages && colcon build --symlink-install --cmake-args -DCMAKE_EXPORT_COMPILE_COMMANDS=ON && mv build/compile_commands.json ..",
            "problemMatcher": []
        },
        {
            "label": "ROS2-Monte-Carlo-Client",
            "type": "shell",
            "command": "source ${workspaceFolder}/packages/install/setup.bash && ros2 launch anytime_monte_carlo action_client.launch.py",
            "problemMatcher": [],
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": true,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": false
            }
        },
